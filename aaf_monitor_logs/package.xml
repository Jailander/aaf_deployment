<?xml version="1.0"?>
<package>
  <name>aaf_monitor_logs</name>
  <version>0.1.0</version>
  <description>A package to monitor that topics are logged to mongodb_store</description>

  <maintainer email="nilsbore@gmail.com">Nils Bore</maintainer>
  <author>Nils Bore</author>

  <url type="website">https://github.com/strands-project/aaf_deployment</url>

  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>std_srvs</build_depend>
  <build_depend>message_generation</build_depend>
  <build_depend>mongodb_store</build_depend>
  <build_depend>mongodb_store_msgs</build_depend>
  <build_depend>python-catkin-pkg</build_depend>
  <build_depend>mongodb</build_depend>
  <build_depend>mongodb-dev</build_depend>

  <run_depend>rospy</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>python-pymongo</run_depend>
  <run_depend>mongodb</run_depend>
  <run_depend>mongodb-dev</run_depend>
  <run_depend>mongodb_store</run_depend>
  <run_depend>mongodb_store_msgs</run_depend>

</package>
